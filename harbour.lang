    <context id="builtin-preprocessor" style-ref="builtin-preprocessor">
      <prefix>\%{statement-start}</prefix>
      <suffix>\b</suffix>
      <keyword>\!|\?|\?\?|@|ACCEPT|CANCEL|CLS|COPY\s+FILE|DELETE\s+FILE|DIR|EJECT|ERASE|INPUT|KEYBOARD|MENU\sTO|QUIT|RENAME|REQUEST|TEXTBLOCK|TYPE|WAIT</keyword>
      <keyword>READ(?:\s+SAVE)?(?:\s+MENU)?</keyword>
      <keyword>CLEAR(?:\s+(ALL|MEMORY|SCREEN|TYPEAHEAD))?</keyword>
      <keyword>SAVE\s+SCREEN(?:\s+TO)?</keyword>
      <keyword>RESTORE\s+SCREEN(?:\s+FROM)?</keyword>
      <keyword>RELEASE(?:\s+ALL(\s+(LIKE|EXCEPT)?))?</keyword>
      <keyword>RESTORE(?:\s+HBV)?(?:\s+FROM)?</keyword>
      <keyword>(FIND|RUN)(?!\s*:?=)</keyword>
      <keyword>SAVE(?:\s+HBV)?(?:\s+(TO|ALL(\s+(LIKE|EXCEPT)?))?)</keyword>
      <keyword>TEXT(?:\s+TO\s+(FILE|PRINTER))?</keyword>
      <keyword>CLOSE(?:\s+(ALL|ALTERNATE|FORMAT))?</keyword>
      <keyword>SET\s+DATE(?:\s+TO)\s+(?:AMERICAN|ANSI|BRITISH|FRENCH|GERMAN|ITALIAN|JAPANESE|USA)</keyword>
      <keyword>SET\s+(ALTERNATE|PRINTER)(?:\s+(ON|OFF|TO))</keyword>
      <keyword>SET\s+DELIMITERS(?:\s+(ON|OFF|TO(\s+DEFAULT)?))</keyword>
      <keyword>SET\s+(BELL|CENTURY|CONFIRM|CONSOLE|CURSOR|ESCAPE|EXACT|FIXED|INTENSITY|SCOREBOARD|WRAP)(?:\s+(?:ON|OFF)?)</keyword>
      <keyword>SET\s+(COLOU?R|DEFAULT|DECIMALS|EPOCH|EVENTMASK|FORMAT|MARGIN|MESSAGE|PATH|PROCEDURE|TYPEAHEAD|VIDEOMODE)\s+TO</keyword>
      <keyword>SET\s+(DATE|TIME)\s+FORMAT(?:\s+TO)?</keyword>
      <keyword>SET\s+DEVICE\s+TO\s+(?:PRINTER|SCREEN)</keyword>
      <keyword>SET\s+(FUNCTION|KEY)</keyword>
      <keyword>SET\s+(DIRCASE|DIRSEPARATOR|EOL|FILECASE)</keyword>
      <keyword>ASSOCIATE\s+CLASS|ENABLE\sCLASS\sTYPE(?:\s+ALL)?</keyword>
    </context>

    <define-regex id="datetime1">\{\^((?:\d{4}[-]\d{2}[-]\d{2},?)?\s*(?:\d\d(?::\d\d(?::\d\d(?:\.\d{1,3})?)?)?(?:\s*[a|p]m)))\}</define-regex>
    <define-regex id="datetime2">\{\^((?:\d{4}[/]\d{2}[/]\d{2},?)?\s*(?:\d\d(?::\d\d(?::\d\d(?:\.\d{1,3})?)?)?(?:\s*[a|p]m)))\}</define-regex>
    <define-regex id="datetime3">\{\^((?:\d{4}[.]\d{2}[.]\d{2},?)?\s*(?:\d\d(?::\d\d(?::\d\d(?:\.\d{1,3})?)?)?(?:\s*[a|p]m)))\}</define-regex>

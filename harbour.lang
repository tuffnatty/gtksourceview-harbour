<language id="harbour" _name="Harbour" version="2.0" _section="Sources">
  <metadata>
    <property name="mimetypes">text/x-prg;application/x-prg</property>
    <property name="globs">*.prg;*.ch</property>
    <property name="line-comment-start">//</property>
    <property name="block-comment-start">/*</property>
    <property name="block-comment-end">*/</property>
  </metadata>

  <styles>
    <style id="comment"           _name="Comment"               map-to="def:comment"/>
    <style id="string"            _name="String"                map-to="def:string"/>
    <style id="preprocessor"      _name="Preprocessor"          map-to="def:preprocessor"/>
    <style id="included-file"     _name="Included File"         map-to="def:string"/>
    <style id="keyword"           _name="Keyword"               map-to="def:keyword"/>
    <style id="function-declaration" _name="Keyword"            map-to="def:keyword"/>
    <style id="operator"          _name="Operator"              map-to="def:operator"/>
    <style id="storage-class"     _name="Storage Class"         map-to="def:type"/>
    <style id="printf"            _name="printf Conversion"     map-to="def:special-char"/>
    <style id="escaped-character" _name="Escaped Character"     map-to="def:special-char"/>
    <style id="floating-point"    _name="Floating point number" map-to="def:floating-point"/>
    <style id="decimal"           _name="Decimal number"        map-to="def:decimal"/>
    <style id="hexadecimal"       _name="Hexadecimal number"    map-to="def:base-n-integer"/>
    <style id="boolean"           _name="Boolean value"         map-to="def:boolean"/>
    <style id="datetime"          _name="Datetime value"        map-to="def:number"/>
    <style id="builtin-class"     _name="Builtin Class"         map-to="def:builtin"/>
    <style id="builtin-function"  _name="Builtin Function"      map-to="def:builtin"/>
    <style id="builtin-preprocessor" _name="Builtin preprocessor command" map-to="def:special-constant"/>
    <style id="line-continue"     _name="Line Continuation"     map-to="def:preprocessor"/>
    <style id="macro-evaluation"  _name="Macro substitution"    map-to="def:special-char"/>
    <style id="error"             _name="Error"                 map-to="def:error"/>
  </styles>

  <default-regex-options case-sensitive="false"/>

  <definitions>

    <define-regex id="preproc-start">^\s*#\s*</define-regex>
    <define-regex id="escaped-character" extended="true">
      \\(                   # leading backslash
      [\\\"\'nrbtfav\?] |   # escaped character
      [0-7]{1,3} |          # one, two, or three octal digits
      x[0-9A-Fa-f]+         # 'x' followed by hex digits
      )
    </define-regex>

    <define-regex id="statement-start">(?:^|(?&lt;=;))\s*</define-regex>

    <context id="printf" style-ref="printf" extend-parent="false">
      <match extended="true">
        \%\%|\%
        (?:[1-9][0-9]*\$)?      # argument
        [#0\-\ \+\'I]*          # flags
        (?:[1-9][0-9]*|\*)?     # width
        (?:\.\-?(?:[0-9]+|\*))? # precision
        (?:hh|ll|[hlLqjzt])?    # length modifier
        [diouxXeEfFgGaAcsCSpnm] # conversion specifier
      </match>
    </context>

    <context id="macro-evaluation" style-ref="macro-evaluation">
      <match>&amp;([A-Za-z_][A-Za-z0-9_]*)\.?</match>
    </context>

    <context id="if0-comment" style-ref="comment">
      <start>\%{preproc-start}if\b\s*0\b</start>
      <end>\%{preproc-start}(endif|else|elif)\b</end>
      <include>
        <context id="if-in-if0">
          <start>\%{preproc-start}if(n?def)?\b</start>
          <end>\%{preproc-start}endif\b</end>
          <include>
            <context ref="if-in-if0"/>
            <context ref="def:in-comment"/>
          </include>
        </context>
        <context ref="def:in-comment"/>
      </include>
    </context>

    <context id="include" style-ref="preprocessor">
      <match extended="true">
        \%{preproc-start}
        (include|import)\s*
        (".*?")
      </match>
      <include>
        <context id="included-file" sub-pattern="2" style-ref="included-file" class="path"/>
      </include>
    </context>

    <context id="preprocessor" style-ref="preprocessor" end-at-line-end="true">
      <start extended="true">
        \%{preproc-start}
        (define|undef|error|pragma|ident|if(n?def)?|else|elif|endif|line|warning|require|stdout|[xy]?(un)?(command|translate))
        \b
      </start>
      <include>
        <context ref="def:line-continue" ignore-style="true"/>
        <context ref="string" ignore-style="true"/>
        <context ref="def:c-like-comment"/>
        <context ref="def:c-like-comment-multiline"/>
      </include>
    </context>

    <context id="xbase-line-comment" style-ref="comment" end-at-line-end="true" class="comment" class-disabled="no-spell-check">
      <start>\&amp;\&amp;|^\s*\*</start>
      <include>
        <context ref="def:in-line-comment"/>
      </include>
    </context>

    <context id="line-continue" style-ref="line-continue">
      <start>;\s*$</start>
      <end>^</end>
    </context>

    <context id="string" style-ref="string" end-at-line-end="true" class="string" class-disabled="no-spell-check">
      <start>"</start>
      <end>"</end>
      <include>
        <context ref="printf"/>
        <context ref="macro-evaluation"/>
       </include>
    </context>
    <context id="string2" style-ref="string" end-at-line-end="true" class="string" class-disabled="no-spell-check">
      <start>'</start>
      <end>'</end>
      <include>
        <context ref="printf"/>
        <context ref="macro-evaluation"/>
       </include>
    </context>
    <context id="string3" style-ref="string" end-at-line-end="true" class="string" class-disabled="no-spell-check">
      <start>(?&lt;=[,(\[+\.\{&amp;&lt;&gt;=]!\w)\s*\[</start>
      <end>\]</end>
      <include>
        <context ref="printf"/>
        <context ref="macro-evaluation"/>
       </include>
    </context>

    <context id="escaped-character" style-ref="escaped-character">
      <match>\%{escaped-character}</match>
    </context>

    <context id="extstring" style-ref="string" end-at-line-end="true" class="string" class-disabled="no-spell-check">
      <start>E"</start>
      <end>"</end>
      <include>
        <context ref="printf"/>
        <context ref="escaped-character"/>
        <context ref="macro-evaluation"/>
      </include>
    </context>
    <context id="extstring2" style-ref="string" end-at-line-end="true" class="string" class-disabled="no-spell-check">
      <start>E'</start>
      <end>'</end>
      <include>
        <context ref="printf"/>
        <context ref="escaped-character"/>
        <context ref="macro-evaluation"/>
       </include>
    </context>

    <!-- http://www.lysator.liu.se/c/ANSI-C-grammar-l.html -->
    <context id="float" style-ref="floating-point">
      <match extended="true">
        (?&lt;![\w\.])
        ((\.[0-9]+ | [0-9]+\.[0-9]*) ([Ee][+-]?[0-9]*)? |
         ([0-9]+[Ee][+-]?[0-9]*))
        [fFlL]?
        (?![\w\.])
      </match>
    </context>

    <context id="hexadecimal" style-ref="hexadecimal">
      <match extended="true">
        (?&lt;![\w\.])
        0[xX][a-fA-F0-9]+
        (?![\w\.])
      </match>
    </context>

    <context id="invalid-hexadecimal" style-ref="error">
      <match extended="true">
        (?&lt;![\w\.])
        0[xX][a-fA-F0-9]*[g-zG-Z][a-zA-Z0-9]*
        (?![\w\.])
      </match>
    </context>

    <context id="decimal" style-ref="decimal">
      <match extended="true">
        (?&lt;![\w\.])
        [0-9]+
        (?![\w\.])
      </match>
    </context>

    <context id="else" once-only="true">
      <prefix>\%{statement-start}</prefix>
      <keyword>ELSE</keyword>
    </context>
    <context id="if-only-keywords">
      <prefix>\%{statement-start}</prefix>
      <keyword>ELSE</keyword>
      <keyword>ELSEIF</keyword>
    </context>
    <context id="otherwise">
      <prefix>\%{statement-start}</prefix>
      <keyword>OTHERWISE</keyword>
    </context>
    <context id="case-only-keywords">
      <prefix>\%{statement-start}</prefix>
      <keyword>CASE</keyword>
    </context>
    <context id="exit">
      <prefix>\%{statement-start}</prefix>
      <keyword>EXIT</keyword>
    </context>
    <context id="loop">
      <prefix>\%{statement-start}</prefix>
      <keyword>LOOP</keyword>
    </context>
    <context id="always" once-only="true">
      <prefix>\%{statement-start}</prefix>
      <keyword>ALWAYS</keyword>
    </context>
    <context id="recover" once-only="true">
      <prefix>\%{statement-start}</prefix>
      <keyword>RECOVER(?:\s+USING)?</keyword>
    </context>

    <context id="loop1">
      <start>\%{statement-start}FOR(?:\s+EACH)?\b</start>
      <end>\%{statement-start}(?:END(?:\s*FOR)?|NEXT)\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="nontoplevel"/>
      </include>
    </context>
    <context id="loop2">
      <start>\%{statement-start}(?:DO\s+)WHILE\b</start>
      <end>\%{statement-start}END(?:\s*DO)?\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="sequence">
      <start>\%{statement-start}BEGIN\s+SEQUENCE(?:\s+WITH)?\b</start>
      <end>\%{statement-start}END(?:\s*SEQUENCE)?\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="recover" style-ref="keyword"/>
        <context ref="always" style-ref="keyword"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="case">
      <start>\%{statement-start}DO\s+CASE\b</start>
      <end>\%{statement-start}END(?:\s*CASE)?\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="case-only-keywords" style-ref="keyword"/>
        <context ref="otherwise" style-ref="keyword"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="switch">
      <start>\%{statement-start}SWITCH\b</start>
      <end>\%{statement-start}END(?:\s*SWITCH)?\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="case-only-keywords" style-ref="keyword"/>
        <context ref="otherwise"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="if">
      <start>\%{statement-start}IF\b(?!\()</start>
      <end>\%{statement-start}END(?:\s*IF)?\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="if-only-keywords" style-ref="keyword"/>
        <context ref="else"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="with">
      <start>\%{statement-start}WITH(\s+OBJECT)?\b</start>
      <end>\%{statement-start}END(?:\s*WITH)?\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="keywords" style-ref="keyword">
      <prefix>\%{statement-start}</prefix>
      <keyword>CALL</keyword>
      <keyword>DO</keyword>
      <keyword>RETURN</keyword>
    </context>

    <context id="operators" style-ref="operator">
      <match>\.AND\.|\.NOT\.|\.OR\.|\$|[-:+*/^%=!&lt;&gt;]=|[-+*/^%&amp;]|&lt;&gt;|&lt;|&gt;</match>
    </context>

    <context id="function-declaration" style-ref="function-declaration">
      <keyword>CRITICAL</keyword>
      <keyword>FUNCTION</keyword>
      <keyword>MESSAGE</keyword>
      <keyword>(?:INIT\s+|EXIT\s+)PROCEDURE</keyword>
    </context>

    <context id="class-only-keywords" style-ref="keyword">
      <prefix>\%{statement-start}</prefix>
      <keyword>(?:SYNC\s+)?(?:CLASS\s*|DECLARED\s+)?METHOD\s+(?:FUNCTION|PROCEDURE)</keyword>
      <keyword>ACCESS</keyword>
      <keyword>ASSIGN</keyword>
      <keyword>CONSTRUCTOR</keyword>
      <keyword>DESTRUCTOR</keyword>
      <keyword>ERROR\s+HANDLER</keyword>
      <keyword>FRIEND\s+(?:CLASS|FUNCTION)</keyword>
      <keyword>MESSAGE</keyword>
      <keyword>ON\s+ERROR(\s+FUNCTION)?</keyword>
      <keyword>OPERATOR</keyword>
    </context>

    <context id="class-only-storage-classes" style-ref="storage-class">
      <prefix>\%{statement-start}</prefix>
      <keyword>(?:CLASS\s*)?(?:DATA|VAR)</keyword>
      <keyword>EXPORT(?:ED)?:</keyword>
      <keyword>EXPORT</keyword>
      <keyword>HIDDEN:</keyword>
      <keyword>HIDDEN</keyword>
      <keyword>PROTECTED:</keyword>
      <keyword>PROTECT</keyword>
      <keyword>VISIBLE:</keyword>
      <keyword>ACCESS</keyword>
      <keyword>ASSIGN</keyword>
      <keyword>CONSTRUCTOR</keyword>
      <keyword>DESTRUCTOR</keyword>
      <keyword>ERROR\s+HANDLER</keyword>
      <keyword>FRIEND\s+(?:CLASS|FUNCTION)</keyword>
      <keyword>MESSAGE</keyword>
      <keyword>ON\s+ERROR(\s+FUNCTION)?</keyword>
      <keyword>OPERATOR</keyword>
    </context>

    <context id="class-declaration">
      <start>\%{statement-start}(?:CREATE\s+)?CLASS\s+(?:[A-Za-z_])</start>
      <end>\%{statement-start}END ?CLASS(?:\s+LOCK(?:ED)?)\b</end>
      <include>
        <context where="start" sub-pattern="0" style-ref="keyword"/>
        <context where="end" sub-pattern="0" style-ref="keyword"/>
        <context ref="class-only-keywords"/>
        <context ref="class-only-storage-classes"/>
        <context ref="nontoplevel"/>
      </include>
    </context>

    <context id="storage-class" style-ref="storage-class">
      <prefix>\%{statement-start}</prefix>
      <keyword>PARAMETERS</keyword>
      <keyword>LOCAL</keyword>
      <keyword>STATIC</keyword>
      <keyword>PRIVATE</keyword>
      <keyword>PUBLIC</keyword>
    </context>

    <context id="boolean" style-ref="boolean">
      <keyword>\.[TFYN]\.</keyword>
    </context>

    <context id="database-command" style-ref="builtin-preprocessor">
      <prefix>\%{statement-start}</prefix>
      <keyword>APPEND\s+(?:BLANK|FROM)</keyword>
      <keyword>AVERAGE</keyword>
      <keyword>CLOSE(?:\s+(DATABASES|FORMAT|INDEX|IDEXES))?</keyword>
      <keyword>COMMIT|CONTINUE|DELETE(\s+TAG)?|LOCATE|PACK|RECALL|REPL(ACE)?|SEEK|SELE(CT)?|USE|ZAP|UNLOCK(?:\s+ALL)?</keyword>
      <keyword>COPY(?:\s+(?:STRUCTURE|EXTENDED|TO))+</keyword>
      <keyword>COUNT(?:\s+TO)?</keyword>
      <keyword>CREATE(?!\s+CLASS\b)</keyword>
      <keyword>DISPLAY</keyword>
      <keyword>FIND</keyword>
      <keyword>GO(?:TO)(?:\s+(?:TOP|BOTTOM))?</keyword>
      <keyword>INDEX\s+ON</keyword>
      <keyword>JOIN(?:\s+WITH)</keyword>
      <keyword>LABEL\s+FORM</keyword>
      <keyword>LIST</keyword>
      <keyword>LOCATE</keyword>
      <keyword>PACK</keyword>
      <keyword>RECALL</keyword>
      <keyword>REINDEX</keyword>
      <keyword>REPORT FORM</keyword>
      <keyword>SEEK</keyword>
      <keyword>SET\s+(DELETED|EXCLUSIVE|SOFTSEEK|UNIQUE)(?:\s+(?:ON|OFF)?)</keyword>
      <keyword>SET\s+(AUTORDER|AUTOSHARE|FILTER|INDEX|MBOCKSIZE|MEMOBLOCK|MFILEEXT|SCOPE(?:BOTTOM|TOP)?)\s+TO</keyword>
      <keyword>SET\s+DESCENDING\s+(?:ON|OFF)</keyword>
      <keyword>SET\s+(STRICTREAD|OPTIMIZE|AUTOPEN)(?:\s+(?:ON|OFF))?</keyword>
      <keyword>SET\s+ORDER\s+TO(?:\s+TAG)?</keyword>
      <keyword>SET DBFLOCKSCHEME</keyword>
      <keyword>SET HARDCOMMIT</keyword>
      <keyword>SET RELATION(?:\s+ADDITIVE)?\s+TO</keyword>
      <keyword>SKIP(?:\s+ALIAS)</keyword>
      <keyword>SORT</keyword>
      <keyword>SUM</keyword>
      <keyword>TOTAL</keyword>
      <keyword>UPDATE(?:\s+FROM)</keyword>
    </context>

